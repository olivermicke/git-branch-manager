{
  "name": "git-branch-manager",
  "version": "0.0.0",
  "description": "Quickly checkout or delete local git branches using a simple CLI.",
  "main": "index.js",
  "author": "Oliver Micke <olivermicke@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/olivermicke/git-branch-manager"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack --mode development && npm run run-dist",
    "run-dist": "node dist/git-branch-manager.js",
    "lint": "eslint --ext .ts --ext .tsx src",
    "test": "jest"
  },
  "dependencies": {
    "ink": "^2.6.0",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/ink": "^2.0.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": ">=1",
    "ink-testing-library": "^1.0.2",
    "jest": "^24.9.0",
    "lint-staged": ">=8",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
